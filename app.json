{
  "expo": {
    "name": "Tappy Toes",
    "slug": "tappy-toes",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tappytoes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Track pregnancy kicks, baby care, feeding, diapers, and medicine reminders. Privacy-focused app with all data stored locally on your device.",
    "primaryColor": "#3F51B5",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "tech.desilva.tappytoes"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3F51B5",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "tech.desilva.tappytoes",
      "versionCode": 5,
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "sounds": [],
          "icon": "./assets/images/android-icon-foreground.png",
          "color": "#3F51B5"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "508786b5-263e-40b2-aaf7-50108600b745"
      }
    }
  }
}
